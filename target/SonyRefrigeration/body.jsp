<div id="htmlcontent_top" class="container">
<ul class="htmlcontent-home clearfix row">
<form id="tmsearchbox" method="get" action="/b2c/contactUs.html">
<li class="htmlcontent-item-1 col-xs-6 col-md-6 col-lg-4">

<table>
	<tr>
		<td>Full Name : </td>
		<td><input class="tm_search_query form-control ac_input" id="fullName" name="fullName" placeholder="Full Name" type="text" style="background: rgba(0, 0, 0, 0.03) none repeat scroll 0 0"></td>
	</tr>
		<tr>
		<td>Contact Number : </td>
		<td><input class="tm_search_query form-control ac_input" id="contactNumber" name="contactNumber" placeholder="Contact Number" type="text" style="background: rgba(0, 0, 0, 0.03) none repeat scroll 0 0"></td>
	</tr>
	<tr>
	<tr>
		<td>Email Id : </td>
		<td><input class="tm_search_query form-control ac_input" id="emailId" name="emailId" placeholder="Email Id" type="text" style="background: rgba(0, 0, 0, 0.03) none repeat scroll 0 0"></td>
	</tr>
</table>
</li>
<li class="htmlcontent-item-2 col-xs-6 col-md-6 col-lg-4">
	<table>
		<tr>
			<td>Subject : </td>
			<td><input class="tm_search_query form-control ac_input" id="subject" name="subject" placeholder="Subject" type="text" style="background: rgba(0, 0, 0, 0.03) none repeat scroll 0 0"></td>
		</tr>
		<tr>
			<td>Message</td>
			<td><textarea rows="4" cols="50" class="tm_search_query form-control ac_input" id="messages" name="messages" placeholder="Type here your message" type="text" style="background: rgba(0, 0, 0, 0.03) none repeat scroll 0 0"></textarea></td>
		</tr>
	</table>

</li>



</ul>
<br/>
<br/>


<button type="submit" name="contactUsSubmit" class="btn btn-default button-search">
<span>Submit</span>
</button>
</form>

</div>